{
  "name": "p-webid",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:wc1": "vue-cli-service build  --no-clean --target wc-async --name p-webid src/components/PWebid.vue",
    "build:wc2": "vue-cli-service build  --no-clean --target wc-async --name p-login src/components/PLogin.vue",
